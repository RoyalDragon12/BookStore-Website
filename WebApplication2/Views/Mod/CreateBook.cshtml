@model WebApplication2.Models.Book


<h1 class="display-5 text-center">Tạo Sách Mới</h1>
@using (Html.BeginForm("CreateBook", "Mod", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    { Html.RenderPartial("_BookFormPartial"); }

    <div class="text-center">
        <input type="submit" value="Tạo" class="btn btn-success btn-lg btn-block mt-5" />
    </div>
    <div class="text-center">
        <a href="@Url.Action("BookManager","Mod")" class="btn btn-danger btn-lg mt-5" style="width:15%">Hủy Bỏ</a>
    </div>
}


