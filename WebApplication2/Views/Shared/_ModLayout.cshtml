    @*Debatable: Wrap with a Container or not ?*@
    <div class="row">
        <div class="col-2 ms-5 me-5">
            @{Html.RenderPartial("_ModLeftSideBarPartial");}
        </div>
        <div class="col-9">
            @RenderBody()
        </div>
