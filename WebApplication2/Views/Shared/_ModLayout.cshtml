    <title>Trang Chủ</title>
    <link href="~/Content/CSSHome/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/CSSHome/price-range.css" rel="stylesheet">
    <link href="~/Content/CSSHome/animate.css" rel="stylesheet">
    <link href="~/Content/CSSHome/main.css" rel="stylesheet">
    <link href="~/Content/CSSHome/responsive.css" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">

