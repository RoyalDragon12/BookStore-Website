@using WebApplication2.Models;
@model IEnumerable<Publisher>

<!DOCTYPE html>


    <ul>
        <li>
            <a href="@Url.Action("Index","Shop")">Tất Cả</a>
        </li>
        @foreach (var item in Model)
        {
            <li><a href="@Url.Action("Index","Shop", new { id = "publisher", q = item.PublisherCode })">@item.PublisherName </a></li>
        }
    </ul>


